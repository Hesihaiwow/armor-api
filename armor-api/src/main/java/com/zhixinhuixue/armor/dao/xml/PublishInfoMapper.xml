<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhixinhuixue.armor.dao.IZSYPublishInfoMapper">
  <resultMap id="BaseResultMap" type="com.zhixinhuixue.armor.model.pojo.PublishInfo">
    <id column="id" jdbcType="BIGINT" property="id" />
    <result column="time" jdbcType="TIMESTAMP" property="time" />
  </resultMap>
  <sql id="Base_Column_List">
    id, time
  </sql>
    <insert id="insertPublishInfo">
      insert into publish_info (id, time)
      values (#{id,jdbcType=BIGINT}, #{time,jdbcType=TIMESTAMP})
    </insert>

  <update id="updatePublishInfo" parameterType="java.util.Map">
    UPDATE publish_info
          SET  time = #{time}
        WHERE id=#{id};
  </update>

  <select id="getPublishInfo" resultType="java.util.Date">
      SELECT time from publish_info WHERE  id = #{id}
  </select>
</mapper>